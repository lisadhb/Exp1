def W(t,h,w):
    W=0.5*t**2-0.2*h+0.1*w-15
    if W>=300:
        print("Sunny")
    elif W>=200 and W<300:
        print("Cloudy")
    elif W>=100 and W<200:
        print("Rainy")
    elif W<100:
        print("Stormy")

t=int(input("Enter the Temperature: "))
h=int(input("Enter the Humidity: "))
w=int(input("Enter the Wind: "))
W(t,h,w)
